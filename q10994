#ë³„

n = int(input())

for i in range(4*n-3):
    print()
    for j in range(4*n-3):
        if i % 2 == 0:
            print("*", end="")
        else:
            if j == 0:
                print("*", end="")
            elif j == 4*n-4:
                print("*", end = "")
            else:
                print("", end = " ")
